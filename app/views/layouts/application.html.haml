!!!
%html
  %head
    %title= title
    = render 'layouts/stylesheets'
    = javascript_include_tag "application"
    = csrf_meta_tags

  %body
    .container_12
      = render 'layouts/header'
              
      #content
        - unless @title.nil?
          %h1= @title
          
        = yield
        
      = render 'layouts/footer'

      = render 'layouts/ga'  if Rails.env.production?

