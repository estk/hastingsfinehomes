!!!
%html
  %head
    %title= title
    = render 'layouts/stylesheets'
    = javascript_include_tag "application"
    = csrf_meta_tags

  %body
    .container_12
      render 'layouts/header'
              
      #content
        = yield
        
      = render 'layouts/footer'
      = render debug(params) if Rails.env.development?
      = render 'layouts/ga'  if Rails.env.production?

